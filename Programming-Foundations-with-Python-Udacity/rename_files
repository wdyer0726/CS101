import os

def rename_files():
	#find file names from a folder
	file_list = os.listdir(r"C:\...") # raw location of file path
	saved_path = os.getcwd() #gets current working directory
	os.chdir(r"C:\...") #change current directory

	#rename each file without numbers
	for file_name in file_list:
		print "Old Name - "+file_name
		print "New Name- " +file_name.translate(None, "0123456789"
    	os.rename(file_name, file_name.translate(None, "0123456789")
    os.chdir(saved_path)
rename_files()
