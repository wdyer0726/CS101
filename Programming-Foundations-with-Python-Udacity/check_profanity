import urllib

def read_text():
	file = open("C:\...") #file location
	filecontents = file.read()
	print(filecontents)
	file.close()
	check_profanity(filecontents)
	
def check_profanity(text_to_check):
	connection = urllib.urlopen("http://www.wdyl.com/profanity?q="+text_to_check)
	output = connection.read()
	print(output)
	connection.close()
	
read_text()
